<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>


    <properties>
        <property name="dialect" value="mysql"/>
        <property name="pageSqlId" value=".*Page$"/>
    </properties>

    <settings>
        <setting name="logImpl" value="LOG4J"/>
    </settings>

    <!-- 命名空间 -->
    <typeAliases>
        <typeAlias alias="User" type="com.youdows.fightting.dto.User"/>
    </typeAliases>

    <plugins>
        <plugin interceptor="com.youdows.fightting.interceptor.PageInterceptor"></plugin>
    </plugins>
</configuration>


